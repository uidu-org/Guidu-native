{
  "compilerOptions": {
    "strictNullChecks": true,
    "isolatedModules": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "app/*": ["./packages/app/*"]
      // "@uidu/native/*": ["./packages/ui/*"]
    },
    "jsx": "react-jsx"
  },
  "extends": "./tsconfig.base.json",
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/next/out",
    "apps/next/.next",
    "apps/next/.tamagui"
  ]
}
